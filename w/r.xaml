<Page x:Class="w.r"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:w"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="r">

    <Grid Background="Bisque">
        <Grid.RowDefinitions>
            <RowDefinition Height="130"/>
            <RowDefinition Height="150"/>
            <RowDefinition Height="auto"/>

        </Grid.RowDefinitions>
        <Label Content="Hello" FontSize="45" FontWeight="Bold" Margin="335,40,335,20" Grid.Row="0"/>
        <ComboBox Name="combo" Height="45" Width="100" Margin="350,50,350,55" Grid.Row="1">
            <ComboBoxItem>
                Admin
            </ComboBoxItem>
            <ComboBoxItem>
                User
            </ComboBoxItem>
        </ComboBox>
        <Button Content="Submit" Style="{StaticResource bu}" Margin="350,24,350,165" Click="Button_Click" Grid.Row="2"/>

    </Grid>
</Page>
